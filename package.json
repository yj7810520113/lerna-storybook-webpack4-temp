{
  "name": "vue-lerna-webpack4-temp",
  "version": "0.0.2",
  "description": "A Vuejs template using Webpack 4 and lerna",
  "main": "index.js",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "lib": "lerna run build --since",
    "storybook": "start-storybook -s ./static -p 8080",
    "build-storybook": "build-storybook -s ./static",
    "dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "prod": "webpack --config build/webpack.config.prod.js",
    "publish": "npm run lib && lerna publish --skip-npm --no-git-reset",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "test:unit": "jest --config test/jest.config.js",
    "test": "npm run test:unit",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "chromatic": "npx chromatic --project-token 08nmnrp7way"
  },
  "private": true,
  "workspaces": [
    "modules/*",
    "modules-test/*"
  ],
  "author": "tracert_it",
  "license": "MIT",
  "dependencies": {
    "chromatic": "^5.4.0",
    "element-ui": "^2.10.1",
    "pbf": "^3.2.1",
    "probe.gl": "^3.0.3",
    "three": "^0.106.2",
    "vector-tile": "^1.3.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {}
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.4.0",
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-notes": "^5.3.12",
    "@storybook/addon-storysource": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/theming": "^5.3.12",
    "@storybook/vue": "^5.3.12",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-preset-vue": "^2.0.2",
    "commitizen": "^3.0.5",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "dexie": "^3.0.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "jest": "^24.5.0",
    "jest-serializer-vue": "^2.0.2",
    "lerna": "^3.15.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "shellhub": "^1.0.0",
    "standard-version": "^4.4.0",
    "storybook-readme": "^5.0.8",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglify-es": "^3.3.9",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.0",
    "vue-server-renderer": "^2.6.10",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
